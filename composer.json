	"authors": [
		{
			"name": "Guilherme Cota"
		},
		{
			"name": "Jéssica Gava"
		},
		{
			"name": "Sady Coimbra"
		},
		{
			"name": "Matheus Rodrigues"
		},
		{
			"name": "João Oliveira"
		},
		{
			"name": "Paloma Brito"
		}
	],
	"autoload": {
		"psr-4": {
			"Efi\\": "src/Efi"
